  "scripts": {
    "boot": "yarn build && docker-compose build && docker-compose start",
    "build": "yarn build:server && yarn build:client",
    "build:client": "yarn webpack",
    "build:server": "babel -d ./server/dist ./server/src",
    "launch:server": "nodemon -w server/dist server/dist/index.js",
    "lint": "eslint",
    "lint:d": "eslint_d",
    "start": "NODE_ENV=development docker-compose build && NODE_ENV=development docker-compose up"
  },
